  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href=/y -o",
    "start-ie": "ng serve --base-href=/y --aot --prod -e=dev",
    "start-iphone": "ng serve --base-href=/y --aot --prod -e=dev --host iphone-test --port 4000",
    "start-proto": "ng serve --env=proto --proxy-config proxy-mock.conf.json",
    "start-proto:meeting": "ng serve --prod --proxy-config proxy-mock.conf.json --build-optimizer=false",
    "start-qa": "node --max_old_space_size=10192 node_modules/@angular/cli/bin/ng serve --base-href=/y --host 0.0.0.0 --disable-host-check --no-live-reload --watch=false --port=4000 --build-optimizer=false --prod -e=dev",
    "build": "node --max_old_space_size=10192 node_modules/@angular/cli/bin/ng build --prod --base-href=/y --deploy-url=/y",
    "build-qa": "node --max_old_space_size=10192 node_modules/@angular/cli/bin/ng build --aot=true --output-hashing=media --target=production --build-optimizer=true --environment=remote --base-href=/y --deploy-url=/y",
    "test": "ng test --watch=false",
    "test-interactive": "ng test",
    "test-coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint --type-check",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.css,.scss}\"",
    "e2e": "ng e2e"
  },
